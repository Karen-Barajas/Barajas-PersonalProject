@model TheMaxieInn.ViewModels.ReservationConfirmationViewModel

@{
    ViewData["Title"] = "Reservation Confirmation";
}

<h3 class="text-center">Reservation Confirmed!</h3>

<p class="text-center">Thank you @Model.OwnerName for booking your reservation for @Model.DogName from @Model.CheckInDate to @Model.CheckOutDate .</p>
<br />

<p class="text-center">We are looking forward to @Model.DogName's stay!</p>
